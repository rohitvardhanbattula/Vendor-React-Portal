{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "vendorportaldest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/uploadattachments$",
      "target": "/uploadattachments",
      "destination": "vendorportaldest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/fileextraction$",
      "target": "/fileextraction",
      "destination": "vendorportaldest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/fetchGSTDetails$",
      "target": "/fetchGSTDetails",
      "destination": "vendorportaldest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/downloadZip/(.*)$",
      "target": "/downloadZip/$1",
      "destination": "vendorportaldest",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/downloadFile/(.*)$",
      "target": "/downloadFile/$1",
      "destination": "vendorportaldest",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "authenticationType": "none"
    }
  ]
}