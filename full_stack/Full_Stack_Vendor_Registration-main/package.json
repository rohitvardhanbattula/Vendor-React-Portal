{
  "name": "VendorPortal",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2.2.0",
    "@sap-cloud-sdk/connectivity": "^3.26.4",
    "@sap-cloud-sdk/http-client": "^4.1.1",
    "@sap-cloud-sdk/mail-client": "^3.24.0",
    "@sap-cloud-sdk/odata-v2": "^4.1.1",
    "@sap-cloud-sdk/resilience": "^3",
    "@sap/cds": "^9.2.0",
    "@sap/xsenv": "^5.4.0",
    "@sap/xssec": "^4",
    "express": "^4",
    "express-fileupload": "^1.5.2",
    "jszip": "^3.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "uninstall": "^0.0.0",
    "update": "^0.7.4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap-cloud-sdk/generator": "^4.1.1",
    "@sap/cds-dk": "^9.3.2",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5",
    "ui5-middleware-simpleproxy": "^3.5.1"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-vendor_portal": "cds watch --open vendor_portal/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy VendorPortal --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "fiori": {
      "preview": true
    },
    "server": {
      "index": true
    },
    "requires": {
      "auth": {
        "kind": "mocked"
      },
      "destination": {
        "kind": "rest"
      },
      "spa_process_destination": {
        "kind": "destination",
        "credentials": {
          "destination": "spa_process_destination"
        }
      },
      "gemini_api": {
        "kind": "destination",
        "credentials": {
          "destination": "gemini_api"
        }
      },
      "gstcheck_api": {
        "kind": "destination",
        "credentials": {
          "destination": "gstcheck_api"
        }
      },
      "sql": {
        "native_hana_associations": false
      },
      "destinations": true,
      "html5-runtime": true,
      "workzone": true,
      "[production]": {
        "auth": "xsuaa"
      },
      "A_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/A_BUSINESS_PARTNER"
      }
    }
  }
}
